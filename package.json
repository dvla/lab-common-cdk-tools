{
  "name": "root",
  "private": true,
  "license": "MIT",
  "description": "DVLA Emerging Tech Lab CDK Tools",
  "scripts": {
    "audit": "lerna exec -- npm audit --audit-level=moderate",
    "build": "lerna run tsc",
    "outdated": "lerna exec -- npm outdated",
    "lint": "lerna run lint",
    "test": "lerna run test",
    "typecheck": "lerna exec -- tsc --noEmit",
    "bootstrap-ci": "lerna bootstrap",
    "version": "lerna version --no-push",
    "publish": "lerna publish from-package --yes"
  },
  "devDependencies": {
    "lerna": "^4.0.0",
    "typescript": "^4.2.4",
    "@types/node": "^14.14.41"
  }
}
