{
  "name": "@dvla/lab-common-cdk",
  "version": "2.3.0",
  "description": "DVLA Emerging Tech Lab common cdk utilities",
  "scripts": {
    "audit": "npm audit --registry=https://registry.npmjs.org --audit-level=moderate",
    "build": "tsc",
    "cdk": "cdk",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint . --ext .js,.ts",
    "outdated": "ncu --errorLevel 2 --color",
    "update": "ncu -u"
  },
  "engines": {
    "node": "^14.18.1",
    "npm": "^8.2.0"
  },
  "dependencies": {
    "lodash": "4.17.21",
    "moment": "2.29.4"
  },
  "peerDependencies": {
    "aws-cdk-lib": "2.29.1",
    "constructs": "10.1.42"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "dist": "/dist",
    "test": "/src/__tests__"
  },
  "files": [
    "dist",
    "!dist/__tests__/"
  ],
  "publishConfig": {
    "registry": "https://nexus.tooling.dvla.gov.uk/repository/npm-private"
