{
    "name": "@dvla/lab-common-cdk",
    "version": "2.40.1",
    "description": "DVLA Emerging Tech Lab common cdk utilities",
    "scripts": {
        "audit": "npm audit --registry=https://registry.npmjs.org --audit-level=moderate",
        "build": "tsc",
        "cdk": "cdk",
        "watch": "tsc -w",
        "test": "jest",
        "lint": "eslint . --ext .js,.ts",
        "outdated": "ncu --errorLevel 2 --color",
        "update": "ncu -u"
    },
    "engines": {
        "node": ">=18",
        "npm": ">=8"
    },
    "dependencies": {
        "lodash": "4.17.21",
        "moment": "2.29.4"
    },
    "peerDependencies": {
        "aws-cdk-lib": "~2.78.0 || ~2.80.0 || ~2.81.0",
        "constructs": "~10.2.31"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "directories": {
        "dist": "/dist",
        "test": "/src/__tests__"
    },
    "files": [
        "dist",
        "!dist/__tests__/"
    ],
    "publishConfig": {
        "registry": "https://nexus.tooling.dvla.gov.uk/repository/npm-private"
    },
    "license": "MIT"
}
